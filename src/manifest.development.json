{
    "manifest_version": 2,
    "name": "chrome-ext",
    "description": "chrome extension",
    "version": "0.0.1",
    "options_page": "options.html",
    "icons":
	{
		"16": "logo.png",
		"48": "logo.png",
		"128": "logo.png"
    },
    "browser_action": {
        "default_title": "chrome-ext",
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}